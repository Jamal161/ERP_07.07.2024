    <script>


     
        $(document).ready(function () {
            $("#loading").hide();

            $("#loginForm").submit(function (event) {
                event.preventDefault();
                handleLogin();
            });

            $(document).on("click", "#logout", function (event) {
                event.preventDefault();
                handleLogout();
            });

            if (isLoggedIn()) {
                updateDropdownMenu();
                updateUserMenu();
            }
        });

        function handleLogin() {
            var userid = $("#UserID").val();
            var empcardno = null;

           
            if (/^[a-zA-Z]-\d+$/.test(userid)) {
                empcardno = userid;
                userid = null; 
            }

            var password = $("#password").val();

            if (!validateInputs(userid, empcardno, password)) {
                return;
            }

            $("#loading").show();
            sendLoginRequest(userid, empcardno, password);
        }

        function validateInputs(userid, empcardno, password) {
            if ((!userid && !empcardno) || !password) {
                alert("All fields are required.");
                return false;
            }
            return true;
        }

        function sendLoginRequest(userid, empcardno, password) {
            $.ajax({
                url: '/Dashboard/gstrGetMpoAreaDevisionLists',
                type: 'POST',
                dataType: 'json',
                data: {
                    "strPassWord": password,
                    "intAdnin": 0,
                    "strUserID": userid,
                    "strCardNo": empcardno,
                    "strDeComID": "0001"
                },
                success: function (response) {
                    handleLoginSuccess(response);
                },
                error: handleLoginError
            });
        }

        function handleLoginSuccess(response) {
            console.log("Full response:", response);
            $("#loading").hide();

            let responseData = Array.isArray(response) ? response[0] : response;

            if (responseData.strResponse === "Yes") {
                console.log("Login successful");
                storeUserData(responseData);
                updateDropdownMenu();
                updateUserMenu();
                redirectUserBasedOnRole();
            } else {
                console.error('Login failed:', responseData.strResponse || "Unknown error");
                alert('Login failed: ' + (responseData.strResponse || "An unknown error occurred"));
            }
        }

        function handleLoginError(xhr, status, error) {
            console.error('Error:', error);
            console.error('XHR:', xhr);
            console.error('Status:', status);
            alert('Login failed. Make sure user name and password are correct.');
            $("#loading").hide();
        }

        function storeUserData(userData) {
            sessionStorage.setItem("userID", userData.strUserID);
            sessionStorage.setItem("userRole", userData.strRole);
            sessionStorage.setItem("userName", userData.strLedgerName);
            sessionStorage.setItem("userCardNo", userData.strCardNo);
        }

        function updateDropdownMenu() {
            var userName = sessionStorage.getItem("userName");
            var userRole = sessionStorage.getItem("userRole");
            var userCardNo = sessionStorage.getItem("userCardNo");
            var userEmpCardNo = sessionStorage.getItem("userEMP_CARD_NO");

            var cardNumberToShow = userEmpCardNo || userCardNo;

            var dropdownMenu = $("#userDropdown");
            dropdownMenu.empty();

            dropdownMenu.append(`<li><a href="#">Welcome, ${userName} (${userRole.trim()})</a></li>`);
        dropdownMenu.append(`<li><a href="#">Card No: ${cardNumberToShow}</a></li>`);
        dropdownMenu.append(`<li role="separator" class="divider"></li>`);
        dropdownMenu.append(`<li><a href="#" id="logout">Logout</a></li>`);
        }

        function updateUserMenu() {
            var userName = sessionStorage.getItem("userName");
            var userRole = sessionStorage.getItem("userRole");
            var userMenu = $("#user-menus");
            userMenu.empty();
            userMenu.append(`<li><a href="#">Welcome, ${userName} (${userRole.trim()})</a></li>`);
        }

        function redirectUserBasedOnRole() {
            var userRole = sessionStorage.getItem("userRole").trim();
            if (userRole === "Admin") {
                window.location.href = "/Dashboard/Index";
            } else if (userRole === "MPO") {
                window.location.href = "/Dashboard/User";
            } else if (userRole === "DH") {
                window.location.href = "/Dashboard/DhUser";
            } else if (userRole === "AH") {
                window.location.href = "/Dashboard/AhUser";
            } else {
                alert('Login failed. Make sure User Role');
            }
        }

        function handleLogout() {
            sessionStorage.clear();
            window.location.href = "/Home/Login";
        }

        function isLoggedIn() {
            return sessionStorage.getItem("userID") !== null;
        }







    </script>




