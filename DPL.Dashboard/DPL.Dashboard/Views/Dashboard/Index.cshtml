        $(document).ready(function () {
            google.charts.load('current', { 'packages': ['corechart'] });
            google.charts.setOnLoadCallback(loadData);

            var currentStatus = 'all';

            $(document).on("click", ".breadcrumb", function () {
                var level = $(this).data("level");
                var teamName = $('.breadcrumb[data-level="team"]').text();
                var zoneName = $('.breadcrumb[data-level="zone"]').text();
                var divName = $('.breadcrumb[data-level="division"]').text();
                var areaName = $('.breadcrumb[data-level="area"]').text();

                switch (level) {
                    case "team":
                        loadData('all');
                        break;
                    case "zone":
                        showZoneDetails(teamName);
                        break;
                    case "division":
                        showDivisionDetails(teamName, zoneName);
                        break;
                    case "area":
                        showAreaDetails(teamName, zoneName, divName);
                        break;
                    case "territory":
                        showTerritoryDetails(teamName, zoneName, divName, areaName);
                        break;
                }
            });

            function loadData(status) {
                $.ajax({
                    url: "/Attendence/mGetKibriadatalist",
                    type: "GET",
                    contentType: 'application/json;charset=utf-8',
                    dataType: "json",
                    success: function (result) {
                        var html = '';
                        $.each(result, function (key, item) {
                            html += '<tr>';
                            html += '<td><button type="button" class="team-button" data-id="' + item.TEAM_NAME + '">' + item.TEAM_NAME + '</button></td>';
                            html += '<td>' + item.NOMPO + '</td>';
                            html += '<td>' + item.Present + '</td>';
                            html += '<td>' + item.LEAVE + '</td>';
                            html += '<td>' + item.ABSENT + '</td>';
                            html += '<td>' + ((item.Present / item.NOMPO) * 100).toFixed(2) + '%</td>';
                            html += '</tr>';
                        });
                        $('.tbody').html(html);
                        drawAttendanceChart(result, 'All Teams');

                        $('.breadcrumb').hide();
                        $('.breadcrumb[data-level="team"]').show().text('Teams');
                        updateBreadcrumbTitle('Teams', 'team');
                    },
                    error: function (errormessage) {
                        alert(errormessage.responseText);
                    }
                });
            }

            $(document).on("click", ".team-button", function () {
                var teamName = $(this).data("id");
                showZoneDetails(teamName);
            });

            function showZoneDetails(teamName) {
                $.ajax({
                    url: "/Attendence/mGetKibriadatazone",
                    type: "POST",
                    contentType: 'application/json;charset=utf-8',
                    dataType: "json",
                    data: JSON.stringify({ TEAM_NAME: teamName }),
                    success: function (result) {
                        var detailsHtml = '';
                        $.each(result, function (key, item) {
                            detailsHtml += '<tr>';
                            detailsHtml += '<td><button type="button" class="zone-button" data-id-team="' + teamName + '" data-id="' + item.ZONE_NAME + '">' + item.ZONE_NAME + '</button></td>';
                            detailsHtml += '<td>' + item.NOMPO + '</td>';
                            detailsHtml += '<td>' + item.Present + '</td>';
                            detailsHtml += '<td>' + item.LEAVE + '</td>';
                            detailsHtml += '<td>' + item.ABSENT + '</td>';
                            detailsHtml += '<td>' + ((item.Present / item.NOMPO) * 100).toFixed(2) + '%</td>';
                            detailsHtml += '</tr>';
                        });
                        $('.tbody').html(detailsHtml);
                        drawAttendanceChart(result, teamName);

                        $('.breadcrumb').hide();
                        $('.breadcrumb[data-level="team"]').show().text('Teams');
                        $('.breadcrumb[data-level="zone"]').show().text(teamName);
                        updateBreadcrumbTitle(teamName, 'zone');
                    },
                    error: function (errormessage) {
                        alert(errormessage.responseText);
                    }
                });
            }

            $(document).on("click", ".zone-button", function () {
                var teamName = $(this).data("id-team");
