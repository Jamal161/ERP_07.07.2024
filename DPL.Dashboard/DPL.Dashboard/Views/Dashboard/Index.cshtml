        $(document).ready(function () {
            var currentStatus = 'all';
            loadData(currentStatus);

            function loadData(status) {
                $.ajax({
                    url: "/Attendence/mGetKibriadataGroupwises",
                    type: "GET",
                    contentType: 'application/json;charset=utf-8',
                    dataType: "json",
                    success: function (result) {
                        var teams = groupBy(result, 'TEAM_NAME');
                        var html = generateHtml(teams, 'team-button', 'TEAM_NAME');
                        $('.card-container').html(html);
                    },
                    error: function (errormessage) {
                        alert(errormessage.responseText);
                    }
                });
            }

            function generateHtml(groups, buttonClass, dataKey) {
                var html = '';
                var count = 0;
                $.each(groups, function (groupName, items) {
                    if (groupName !== null && groupName.trim() !== '') {
                        if (count % 4 === 0) {
                            html += '<div class="row">';
                        }
                        html += '<div class="col-sm-3">';
                        html += '<div class="card-product" style="border-radius: 5px; margin-left:10px; margin-bottom: 10px;width:300px;">';
                        html += `<div class="card-header ${buttonClass}" data-id="${groupName}" style="background-color: #fff; padding-top: 15px; padding-left: 10px; font-size: 20px; font-weight: bold; cursor: pointer; width: 300px;">${groupName}</div>`;
                        html += '<div class="card-header" style="background-color: #fff; padding-left: 28px; font-size: 14px; border-bottom: 1px solid #ddd;"><strong>Role:</strong> <strong style="margin-left: 160px">Default:</strong></div>';
                        html += '<div class="card-body" style="padding: 20px; background-color:#fff">';
                        var roles = groupBy(items, 'Role');
                        $.each(roles, function (roleName, roleItems) {
                            if (roleName !== null && roleName.trim() !== '' && roleItems.length > 0) {
                                var presentCount = roleItems.reduce((acc, obj) => acc + obj.Present, 0);
                                html += '<div class="card-section" style="margin-bottom: 15px;">';
                                html += `<p style="margin-left:10px;"> <span>${roleName}</span> <span style="padding-right: 30px; float: right;">${presentCount}</span></p>`;
                                html += '</div>';
                            }
                        });
                        html += '</div>';
                        html += '</div>';
                        html += '</div>';
                        count++;
                        if (count % 4 === 0 || count === Object.keys(groups).length) {
                            html += '</div>';
                        }
                    }
                });
                return html;
            }

            function groupBy(array, key) {
                return array.reduce((acc, obj) => {
                    var property = obj[key];
                acc[property] = acc[property] || [];
                acc[property].push(obj);
                return acc;
            }, {});
        }

    $(document).on("click", ".team-button", function () {
            var teamName = $(this).data("id");
            showZoneDetails(teamName);
        });

        function showZoneDetails(teamName) {
            $.ajax({
                url: "/Attendence/mGetKibriadataGroupzonewises",
                type: "POST",
                contentType: 'application/json;charset=utf-8',
                dataType: "json",
                data: JSON.stringify({ TEAM_NAME: teamName }),
                success: function (result) {
                    var zones = groupBy(result, 'ZONE_NAME');
                    var html = generateHtml(zones, 'zone-button', 'ZONE_NAME');
                    $('.card-container').html(html);
                },
                error: function (errormessage) {
                    alert(errormessage.responseText);
                }
            });
        }

        $(document).on("click", ".zone-button", function () {
            var teamName = $(this).data("id-team");
            var zoneName = $(this).data("id");
            showDivisionDetails(teamName, zoneName);
        });

        function showDivisionDetails(teamName, zoneName) {
            $.ajax({
                url: "/Attendence/mGetKibriadataGroupdivwises",
                type: "POST",
                contentType: 'application/json;charset=utf-8',
                dataType: "json",
                data: JSON.stringify({ TEAM_NAME: teamName, ZONE_NAME: zoneName }),
                success: function (result) {
                    var divisions = groupBy(result, 'DIVISION');
                    var html = generateHtml(divisions, 'division-button', 'DIVISION');
                    $('.card-container').html(html);
                },
                error: function (errormessage) {
                    alert(errormessage.responseText);
                }
            });
        }

        $(document).on("click", ".division-button", function () {
            var teamName = $(this).data("id-team");
            var zoneName = $(this).data("id-zone");
            var divName = $(this).data("id");
            showAreaDetails(teamName, zoneName, divName);
        });

        function showAreaDetails(teamName, zoneName, divName) {
            $.ajax({
                url: "/Attendence/mGetKibriadataGroupareawises",
                type: "POST",
                contentType: 'application/json;charset=utf-8',
                dataType: "json",
                data: JSON.stringify({ TEAM_NAME: teamName, ZONE_NAME: zoneName, MPO_DIV: divName }),
                success: function (result) {
                    var areas = groupBy(result, 'MPO_AREA');
                    var html = generateHtml(areas, 'area-button', 'MPO_AREA');
                    $('.card-container').html(html);
                },
                error: function (errormessage) {
                    alert(errormessage.responseText);
                }
            });
        }

        $(document).on("click", ".area-button", function () {
            var teamName = $(this).data("id-team");
            var zoneName = $(this).data("id-zone");
            var divName = $(this).data("id-div");
            var areaName = $(this).data("id");
            showTerritoryDetails(teamName, zoneName, divName, areaName);
        });

        function showTerritoryDetails(teamName, zoneName, divName, areaName) {
            $.ajax({
                url: "/Attendence/mGetKibriadatamarket",
                type: "POST",
                contentType: 'application/json;charset=utf-8',
                dataType: "json",
                data: JSON.stringify({ TEAM_NAME: teamName, ZONE_NAME: zoneName, DIVISION: divName, MPO_AREA: areaName }),
                success: function (result) {
                    var markets = groupBy(result, 'TERRITORRY_NAME');
                    var html = generateHtml(markets, 'territory-button', 'TERRITORRY_NAME');
                    $('.card-container').html(html);
                },
                error: function (errormessage) {
                    alert(errormessage.responseText);
                }
            });
        }

        $(document).on("click", ".territory-button", function () {
            var teamName = $(this).data("id-team");
            var zoneName = $(this).data("id-zone");
            var divName = $(this).data("id-div");
            var areaName = $(this).data("id-area");
            var marketName = $(this).data("id");
            showRouteDetails(teamName, zoneName, divName, areaName, marketName);
        });

        function showRouteDetails(teamName, zoneName, divName, areaName, marketName) {
            $.ajax({
                url: "/Attendence/mGetKibriadatamarket",
                type: "POST",
                contentType: 'application/json;charset=utf-8',
                dataType: "json",
                data: JSON.stringify({ TEAM_NAME: teamName, ZONE_NAME: zoneName, DIVISION: divName, MPO_AREA: areaName, TERRITORRY_NAME: marketName }),
                success: function (result) {
                    var marketRoutes = groupBy(result, 'MARKET_NAME');
                    var html = generateHtml(marketRoutes, '', 'MARKET_NAME');
                    $('.card-container').html(html);
                },
                error: function (errormessage) {
                    alert(errormessage.responseText);
                }
            });
        }
        });





