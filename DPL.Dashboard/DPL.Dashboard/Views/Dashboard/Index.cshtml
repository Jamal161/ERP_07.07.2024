            function showDivisionDetails(teamName, zoneName) {
                $.ajax({
                    url: "/Attendence/mGetKibriadatadiv",
                    type: "POST",
                    contentType: 'application/json;charset=utf-8',
                    dataType: "json",
                    data: JSON.stringify({ TEAM_NAME: teamName, ZONE_NAME: zoneName }),
                    success: function (result) {
                        var detailsHtml = '';
                        $.each(result, function (key, item) {
                            detailsHtml += '<tr>';
