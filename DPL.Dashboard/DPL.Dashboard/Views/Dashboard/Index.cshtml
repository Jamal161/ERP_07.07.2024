                            detailsHtml += '<td>' + item.NOMPO + '</td>';
                            detailsHtml += '<td>' + item.Present + '</td>';
                            detailsHtml += '<td>' + item.LEAVE + '</td>';
                            detailsHtml += '<td>' + item.ABSENT + '</td>';
                            detailsHtml += '<td>' + ((item.Present / item.NOMPO) * 100).toFixed(2) + '%</td>';
                            detailsHtml += '</tr>';
                        });
                        $('.tbody').html(detailsHtml);
                        drawAttendanceChart(result, zoneName);

                        $('.breadcrumb').hide();
                        $('.breadcrumb[data-level="team"]').show().text('Teams');
                        $('.breadcrumb[data-level="zone"]').show().text(teamName);
                        $('.breadcrumb[data-level="division"]').show().text(zoneName);
